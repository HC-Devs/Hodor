{
  "install": {
    "include": [
      "^package\\.json$",
      "^\\.env$"
    ]
  },
  "restart": {
    "exclude": [
      "^public/",
      "^dist/",
      "^commands/",
      "^crons/",
      "^events/",
      "^classes/",
      "\\.md$"
    ],
    "include": [
      "\\.js$",
      "\\.json",
      "\\.ts"
    ]
  },
  "throttle": 60001
}